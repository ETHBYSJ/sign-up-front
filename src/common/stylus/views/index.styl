.index_main
  height 100%      
  min-height: 700px
  min-width: 900px
  background-color: #ebebeb
  .index_container
    position relative
    .index_wapper
      height: 100% 
      width: 900px
      margin: 0 auto
      padding-top 14px
      .index_left_wapper
        position fixed
        width 90px
        .index_left_panel
          .index_left_option
            text-align: center
            line-height 40px
            border-top 1px solid #dddddd
            font-size 18px
            font-weight: bold
            color #064b80
            cursor: pointer
            &.inactive
              cursor not-allowed
            &.active
              border-right 2px solid #1b98cd
      .index_right_wapper
        float right
        width: 790px
        .index_right_logout
          color #999999
          text-align: center
          font-size 20px
        .index_right_login
          background-color: #f7f7f7
          height 100%
          padding 15px 44px
          .index_right_login1
            height 100%
            .index_right_title
              text-align: center
              line-height: 45px
              color #094275
              border-bottom: 1px solid #e8e8e8
            .index_right_phone
              height 140px
              padding 55px 110px
              border-bottom: 1px solid #e8e8e8
              font-size 14px
              line-height 26px
              .index_right_num
                float left
                border 2px solid #f7f7f7
              .index_right_btn
                float right
                width 64px
                text-align: center
                border 2px solid green
                border-radius: 8px
                font-weight: bold
                color #064b80
                cursor pointer
                &:hover
                  opacity: 0.8
            .index_right_inputs
              padding 24px 110px
            .index_right_next_box
              height: 100px
              padding 40px 0 30px
              .index_right_next_btn
                color: rgba(0, 0, 0, 0.26)
                background-color: rgba(0, 0, 0, 0.12);
                height 35px
                width 80px
                line-height: 35px
                font-size 13px
                border-radius: 4px
                text-align: center
                margin 0 auto
                box-shadow: 1px 1px 3px 0px #cccccc
                cursor not-allowed
                font-weight: bold
                &.active
                  background-color #064b80
                  cursor pointer
                  color white
                  &:hover
                    opacity: 0.8
          .index_right_login2
            height: 100%
            .index_right_title
              text-align: center
              line-height: 45px
              color #064b80
              border-bottom: 1px solid #e8e8e8
            .index_right_content
              padding 14px 100px
              .el-table
                background-color: transparent !important
                .el-table__header-wapper
                  display none
              .pagination_box
                text-align: center
            .index_right_btn_box
              height 35px
              .index_right_btn
                height 35px
                width 80px
                line-height: 35px
                font-size 13px
                border-radius: 4px
                text-align: center
                margin 0 auto
                box-shadow: 1px 1px 3px 0px #cccccc
                font-weight: bold
                background-color #064b80
                cursor pointer
                color white
                &:hover
                  opacity: 0.8
          .index_right_login3
            height: 100%
            .index_right_title
              text-align: center
              line-height: 45px
              color #064b80
              border-bottom: 1px solid #e8e8e8
            .sample_file_wapper
              height 200px
              padding 50px 0
              line-height: 20px
              font-weight: 300
              font-size 18px
              .sample_file_name
                color blue
                text-decoration: underline
                cursor pointer
              .sample_file_intro
                margin 8px 0 0 80px
                font-size 15px
            .upload_file_wapper
              height 150px
              padding 20px 0 30px
              line-height: 30px
              font-weight: 300
              font-size 18px
              .upload_file_title
                float left
              .upload_file_right
                width: 300px
                margin-left 4px
                float left
                .file_uploader
                  .upload_file_btn
                    display inline-block
                    font-size 14px
                    font-weight: bold
                    color purple
                    border-radius: 4px
                    border 1px solid purple
                    height 30px
                    width: 80px
                    text-align: center
                .upload_file_intro
                  font-size 15px